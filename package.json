{
  "name": "kiwi-lang",
  "displayName": "kiwi-lang",
  "description": "Syntax highlighting for the Kiwi Programming Language",
  "version": "0.0.2",
  "publisher": "fuseraft",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fuseraft/kiwi-vscode.git"
  },  
  "contributes": {
    "languages": [{
      "id": "kiwi",
      "aliases": ["Kiwi", "kiwi"],
      "extensions": [".kiwi"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "kiwi",
      "scopeName": "source.kiwi",
      "path": "./syntaxes/kiwi.tmLanguage.json"
    }]
  }
}
